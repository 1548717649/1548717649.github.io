#include<stdio.h>
int main()
{
	int n = 0;
	char m;
	scanf("%d %c", &n, &m);
	int i = 0;
	float k = n;
	int p = n / 2;
	k = k / 2;
	k = k - p;
	if (k >= 0.5)
	{
		p ++ ;
	}
	for (p; p > 0; p--)
	{
		for (int l=n; l > 0; l--)
		{
			if (i% n == 0&&i!=0)
			{
				printf("\n");
			}
			printf("%c", m);
			i++;
		}
	}
}
