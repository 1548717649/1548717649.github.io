//洛希极限
#include<stdio.h>
int main()
{
	float p,r;
	float b;
	int f;
	scanf("%f %d %f",&p,&f,&r);
	if(f==0)
	{
		b=p*2.455;
		if(b>r)
		printf("%.2f T_T\n",b);
		else
		printf("%.2f ^_^\n",b);
	}
	if(f==1)
	{
		b=p*1.26;
		if(b>r)
		printf("%.2f T_T\n",b);
		else
		printf("%.2f ^_^\n",b);
	}
	return 0;
 } 

//强迫症
#include<stdio.h>
#include<string.h>
int main()
{
	char arr[10];
	scanf("%s", &arr);
	int a = strlen(arr);
	if (a<=4)
	{
		if (arr[0] == '2')
		{
			if (arr[1] == '0' || arr[1] == '1')
			{
				printf("20%c%c-%c%c", arr[0], arr[1], arr[2], arr[3]);
			}
			else
			{
				printf("19%c%c-%c%c", arr[0], arr[1], arr[2], arr[3]);
			}
		}
		else if (arr[0] == '0' || arr[0] == '1')
		{
			printf("20%c%c-%c%c", arr[0], arr[1], arr[2], arr[3]);
		}
		else
		{
			printf("19%c%c-%c%c", arr[0], arr[1], arr[2], arr[3]);
		}
	}
	else
	{
		printf("%c%c%c%c-%c%c", arr[0], arr[1], arr[2], arr[3], arr[4], arr[5]);
	}
}
